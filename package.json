{
  "name": "assetwatcher",
  "version": "1.3.6",
  "description": "Smarter file change watcher supporting executing commands for files individually and ignoration of imported files.",
  "keywords": [
    "asset-watch",
    "assetwatch",
    "asset",
    "js-watch",
    "jswatch",
    "jswatcher",
    "sass-watch",
    "sasswatch",
    "sasswatcher",
    "node-sass",
    "libsass",
    "sass",
    "scss",
    "foreach",
    "each",
    "watch"
  ],
  "main": "assetwatcher.js",
  "bin": {
    "assetwatcher": "./assetwatcher.js"
  },
  "author": "danielkalen",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielkalen/assetwatcher"
  },
  "license": "MIT",
  "scripts": {
    "watch": "nodemon -e coffee -w ./src -x 'npm run compile'",
    "compile": "coffee -o ./ -c src/assetwatcher.coffee && npm run prependbin && npm run makeexec",
    "prependbin": "echo '#!/usr/bin/env node' | cat - assetwatcher.js > tmp.js && mv tmp.js assetwatcher.js",
    "makeexec": "chmod +x assetwatcher.js",
    "copytopublic": "cp -r -f ./ ../../sandbox-public/assetwatcher"
  },
  "dependencies": {
    "bluebird": "^3.4.3",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.0",
    "fireworm": "^0.7.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.3",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "coffee-script": "^1.10.0",
    "create-output-stream": "0.0.1",
    "istanbul": "^0.4.5",
    "lcov-parse": "0.0.10",
    "mocha": "^3.0.2",
    "nodemon": "^1.8.1",
    "request": "^2.74.0",
    "svg2png": "^3.0.1"
  }
}
